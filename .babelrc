{
  "presets": [
    "env",
    "react",
    "stage-1"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    },
    "production": {
      "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
