    "url-loader": "^0.6.2",
    "webpack": "^3.12.0"
  },
  "peerDependencies": {
    "react": ">0.14.0 || >15.0.0",
    "react-dom": ">0.14.0 || >15.0.0"
  },
  "keywords": [
    "react",
    "carousel",
    "react-carousel",
    "react-component",
    "component",
    "image",
    "image-gallery",
    "image-slider",
    "responsive",
    "gallery",
    "rwd"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brainhubeu/react-carousel"
  },
  "main": "./lib/react-carousel.js"
