    "url-loader": "^0.5.9",
    "webpack": "^3.0.0"
  },
  "peerDependencies": {
    "react": ">0.14.0 || >15.0.0",
    "react-dom": ">0.14.0 || >15.0.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/brainhubeu/react-carousel"
  },
  "main": "./lib/react-carousel.js"
}
